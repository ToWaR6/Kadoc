<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<BorderPane prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ListOverViewController">
   <center>
      <StackPane>
         <children>
            <TableView>
               <columns>
                  <TableColumn prefWidth="75.0" text="Classes" />
                  <TableColumn prefWidth="75.0" text="#Questions" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </StackPane>
   </center>
   <top>
      <Group BorderPane.alignment="CENTER">
         <children>
            <HBox spacing="10.0">
               <children>
                  <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.0">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </ProgressBar>
                  <JFXButton onAction="#handleProgressBar" style="-fx-background-color: #cc2e2c;" text="Button" textFill="WHITE" wrapText="true">
                     <HBox.margin>
                        <Insets bottom="-5.0" />
                     </HBox.margin>
                  </JFXButton>
               </children>
               <padding>
                  <Insets bottom="10.0" top="20.0" />
               </padding>
            </HBox>
         </children>
      </Group>
   </top>
</BorderPane>
